{
  "name": "kanban-board",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start-front": "wait-on http://localhost:3001 && vite",
    "start-server": "npx json-server db.json --port 3001",
    "dev": "concurrently --kill-others-on-fail \"npm run start-server\" \"npm run start-front\"",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "prepare": "husky",
    "typecheck": "tsc",
    "check": "npm run test && npm run lint && npm run typecheck"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.28.6",
    "npm-run-all": "^4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.22.3"
  },
  "devDependencies": {
    "@feature-sliced/eslint-config": "^0.1.0-beta.6",
    "@tanstack/eslint-plugin-query": "^5.28.6",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.2",
    "@testing-library/user-event": "14.5.2",
    "@types/react": "18.2.69",
    "@types/react-dom": "18.2.22",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.3.1",
    "@vitejs/plugin-react": "4.2.1",
    "concurrently": "^9.1.1",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-boundaries": "^4.2.0",
    "eslint-plugin-filename-rules": "^1.3.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "0.4.6",
    "eslint-plugin-vitest-globals": "1.5.0",
    "husky": "^9.0.11",
    "jsdom": "24.0.0",
    "prettier": "3.2.5",
    "typescript": "5.4.3",
    "vite": "5.4.8",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "1.4.0",
    "wait-on": "^8.0.1"
  }
}
